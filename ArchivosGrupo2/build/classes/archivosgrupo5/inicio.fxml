<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="mnBar" onDragDetected="#salir" prefHeight="400.0" prefWidth="640.0" style="-fx-background-color: #E3C8B9;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="archivosgrupo5.controladorArchivos">
   <children>
      <HBox prefHeight="34.0" prefWidth="640.0" style="-fx-background-color: #C46D5E;">
         <children>
            <Button blendMode="LIGHTEN" mnemonicParsing="false" onAction="#abrirDirectorio" prefHeight="37.0" prefWidth="122.0" style="-fx-background-color: #F3C98B; -fx-background-radius: 0;" text="Abrir directorio">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button alignment="CENTER" blendMode="LIGHTEN" mnemonicParsing="false" prefHeight="34.0" prefWidth="122.0" style="-fx-background-color: #DAA588; -fx-background-radius: 0;" text="Guardar captura">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
            <Button fx:id="btnSalir" mnemonicParsing="false" onAction="#salir" prefHeight="33.0" prefWidth="66.0" style="-fx-background-color: #F56960; -fx-background-radius: 0;" text="Salir" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font></Button>
         </children>
      </HBox>
   </children>
</VBox>
